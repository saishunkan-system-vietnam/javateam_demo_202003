<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
        <settings>
            <setting name="logImpl" value="LOG4J"/>
        </settings>
        <typeAliases>
            <typeAlias alias="AppRole" type="vn.de.example.parkingregistration.model.AppRole"/>
            <typeAlias alias="AppUser" type="vn.de.example.parkingregistration.model.AppUser"/>
            <typeAlias alias="UserRole" type="vn.de.example.parkingregistration.model.UserRole"/>
            <typeAlias alias="Student" type="vn.de.example.parkingregistration.model.Student"/>
            <typeAlias alias="Subject" type="vn.de.example.parkingregistration.model.Subject"/>
            <typeAlias alias="StudentSubject" type="vn.de.example.parkingregistration.model.StudentSubject"/>
        </typeAliases>
        <typeHandlers>
             <typeHandler handler="org.apache.ibatis.type.LocalDateTimeTypeHandler" />
        </typeHandlers>
        <mappers>
            <mapper resource="xmlmapper/AppRoleModelMapper.xml"/>
            <mapper resource="xmlmapper/AppUserModelMapper.xml"/>
            <mapper resource="xmlmapper/UserRoleModelMapper.xml"/>
            <mapper resource="xmlmapper/StudentModelMapper.xml"/>
            <mapper resource="xmlmapper/SubjectModelMapper.xml"/>
            <mapper resource="xmlmapper/StudentSubjectModelMapper.xml"/>
        </mappers>
</configuration>